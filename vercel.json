{
    "version": 2,
    "name": "hng-backend-task2",
    "public": true,
    "builds": [
        {
            "src": "app.js", 
            "use": "@vercel/node"
        }
    ],
    "functions": {
        "handler": {
            "maxDuration": 30
        }
    },
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/app.js"
        }
    ]
}
